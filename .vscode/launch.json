    "configurations": [
        {
            "name": "Python: Debug Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "autoReload": {
                "enable": true
            },
            "cwd": "${command:cmake.buildDirectory}",
            "env": {
                "PYTHONPATH": "${command:cmake.buildDirectory}/python/lib",
                "HOLOSCAN_LOG_LEVEL": "DEBUG",
                "HOLOSCAN_EXECUTOR_LOG_LEVEL": "INFO",
                "HOLOSCAN_TESTS_DATA_PATH": "${workspaceFolder}/${env:HOLOSCAN_PUBLIC_FOLDER}/tests/data",
                "HOLOSCAN_INPUT_PATH": "${workspaceFolder}/${env:HOLOSCAN_PUBLIC_FOLDER}/data",
            },
        },
        {
            "name": "Python C++ Debug",
            "type": "pythoncpp",
            "request": "launch",
            "pythonLaunchName": "Python: Debug Current File",
            "cppConfig": "default (gdb) Attach",
        },
        {
            "name": "(gdb) Python: Debug Current File",
            "type": "cppdbg",
            "request": "launch",
            "program": "/usr/bin/bash",
            "args": [
                "${workspaceFolder}/${env:HOLOSCAN_PUBLIC_FOLDER}/.vscode/debug_python",
                "${file}",
            ],
            "stopAtEntry": false,
            "cwd": "${command:cmake.buildDirectory}",
            "environment": [
                {
                    "name": "HOLOSCAN_LOG_LEVEL",
                    "value": "DEBUG"
                },
                {
                    "name": "HOLOSCAN_EXECUTOR_LOG_LEVEL",
                    "value": "INFO"
                },
                {
                    "name": "PYTHONPATH",
